<div class="row">
  <div class="col-sm-12 text-center">
    <h1>Questions</h1>
  </div>
</div>
<div class="row">
  <div class="col-sm-12 text-center">
    <% @questions.each do |question| %>
      <h4><%= link_to question.title, question_path(question) %></h4>
    <% end %>
    <br />
    <%= link_to 'Add Question', new_question_path, class: "btn btn-primary"%>
  </div>
</div>
